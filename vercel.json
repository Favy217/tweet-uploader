{
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        },
        {
            "source": "/",
            "destination": "/frontend/tweet-uploader.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
	
	"rewrites": [
    {
      "source": "/callback",
      "destination": "/callback/index.html"
    }
  ]
}
