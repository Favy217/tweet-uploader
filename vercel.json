{
    "rewrites": [
        {
            "source": "/frontend/callback.html",
            "destination": "/frontend/callback.html"
        },
        {
            "source": "/(.*)",
            "destination": "/frontend/$1"
        },
        {
            "source": "/",
            "destination": "/frontend/index.html"
        }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}
